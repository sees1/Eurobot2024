<?xml version="1.0"?>
<launch>

  <rosparam file="$(find aruco_robot)/config/common.yaml" command="load" ns="" />

    <!-- Map server -->
    <!-- <arg name="map_file" default="$(find aruco_robot)/map/map_1.yaml"/> -->

    <!-- <node pkg="map_server" name="map_server"  type="map_server" args="$(arg map_file)">
      <param name="frame_id" value="earth"/>
    </node> -->


    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <rosparam file="$(find aruco_robot)/costmap_cfg/costmap_common_params.yaml"    command="load" ns="global_costmap" />
        <rosparam file="$(find aruco_robot)/costmap_cfg/costmap_common_params.yaml"    command="load" ns="local_costmap" />
        <rosparam file="$(find aruco_robot)/costmap_cfg/local_costmap_params.yaml"     command="load" />
        <rosparam file="$(find aruco_robot)/costmap_cfg/global_costmap_params.yaml"    command="load" />
        <rosparam file="$(find aruco_robot)/nav_cfg/neo_local_planner_params.yaml"       command="load" />
        <rosparam file="$(find aruco_robot)/nav_cfg/move_base_params.yaml"               command="load" />

        <remap from="odom" to="odometry/filtered_odom"/>
    </node>

</launch>